        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.2",
        "webpack-parallel-uglify-plugin": "^2.0.0"
    },
    "jest": {
        "setupFiles": [
            "raf/polyfill",
            "<rootDir>/test/helpers/enzyme-setup.js"
        ],
        "testPathIgnorePatterns": [
            "src/test.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
            "editor-msgs(\\.js)?$": "<rootDir>/test/__mocks__/editor-msgs-mock.js"
        }
    },
    "build": {
        "productName": "NEW-AI极睿",
        "appId": "com.NEW-AI.eletest",
        "copyright": "cd©20211209",
        "directories": {
            "buildResources": "./",
            "output": "pack"
        },
        "afterPack": "./scripts/afterPack.js",
        "extraResources": [
            {
                "from": "./resources/LB_FWLIB",
                "to": "./LB_FWLIB"
            },
            {
                "from": "./resources/zadig.exe",
                "to": "./zadig.exe"
            },
            {
                "from": "./resources/ByteCode",
                "to": "./ByteCode"
            },
            {
                "from": "build",
                "to": "app.asar.unpacked"
            },
            {
                "from": "NEW-AI极睿智能编程套装说明书.pdf",
                "to": "../"
            },
            {
                "from": "NEW-AI极睿模块说明-V1.3.2.pdf",
                "to": "../"
            },
            {
                "from": "README.md",
                "to": "README.md"
            },
            {
                "from": "scripts",
                "to": "./scripts",
                "filter": [
                    "**/hotVersion.json"
                ]
            }
        ],
        "files": [
            "node_modules/**/*",
            "scripts/**/*",
            "src/utils/storeChildProcess.js",
            "src/utils/StoreUtil.js",
            "src/config/json/**/*",
            "main.js",
            "loadWin.js",
            "preload.js",
            "update.js",
            "main-with-cache.js"
        ],
        "fileAssociations": [
            {
                "ext": "lbs",
                "name": "LBS File",
                "icon": "./favicon.ico"
            }
        ],
        "nsis": {
            "oneClick": false,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "./favicon.ico",
            "uninstallerIcon": "./favicon.ico",
            "installerHeaderIcon": "./favicon.ico",
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "shortcutName": "NEW-AI极睿",
            "runAfterFinish": false,
            "perMachine": false,
            "deleteAppDataOnUninstall": true
        },
        "publish": [
            {
                "provider": "generic",
                "url": "https://zsff.drluck.club/ATC"
            }
        ],
        "win": {
            "icon": "./favicon.ico",
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}
